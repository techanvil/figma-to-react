{
  "name": "figma-to-react-plugin",
  "version": "1.0.0",
  "description": "Figma plugin for the Figma to React Bridge",
  "main": "dist/code.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:watch": "webpack --mode=development --watch",
    "dev": "npm run build:watch",
    "dev:fast": "webpack --mode=development --watch --progress",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && mkdir -p dist"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@figma/plugin-typings": "^1.90.0",
    "@types/node": "^20.10.0",
    "babel-loader": "^9.1.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "keywords": [
    "figma",
    "plugin",
    "react",
    "design-tokens"
  ],
  "author": "",
  "license": "MIT"
}
